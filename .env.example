# SenseForge Environment Configuration
# Copy this file to .env and fill in your API keys

# API Keys (Optional - leave empty for mock mode)
CAMBRIAN_API_KEY=
LETTA_API_KEY=
AMBIENT_API_KEY=

# Operating Mode
SENSEFORGE_MODE=mock  # Options: mock, live

# Dashboard Settings
SENSEFORGE_REFRESH_INTERVAL=5  # seconds

# Logging
SENSEFORGE_LOG_LEVEL=INFO  # Options: DEBUG, INFO, WARNING, ERROR

# Model Checkpoints
SENSEFORGE_CHECKPOINT_PATH=checkpoints/jepa_model.pth
